using System;

class MainClass {
  public static void Main (string[] args)
   {
//Variável Real: largura, comprimento e área
  double larg,compri,area;
//Escreva: Digite o número da largura 
Console.Clear();
Console.Write("Digite o número da largura:");
//Leia largura
larg = double.Parse(Console.ReadLine());
//Escreva: Digite o número do comprimento
Console.Write("Digite o número do comprimento: ");
//Leia compri
compri = double.Parse(Console.ReadLine());
//Area = largura * comprimento
area = compri * larg;
Console.WriteLine("A área {0}",area);
}
}
